    "@types/papaparse": "^5.3.14",
    "@types/ws": "^8.5.10",
    "nock": "14.0.0-beta.3",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.8",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "papaparse": "^5.4.1",
    "ws": "^8.16.0"
  },
  "publishConfig": {
